

!URxvt.font:           xft:Roboto Mono for Powerline:regular:pixelsize=14:antialias=true,xft:Droid Sans Mono for Powerline:pixelsize=14:antialias=true
!URxvt.imFont:         xft:Roboto Mono for Powerline:regular:pixelsize=14:antialias=true,xft:Droid Sans Mono for Powerline:pixelsize=14:antialias=true
!URxvt.boldFont:       xft:Roboto Mono for Powerline:regular:pixelsize=14:antialias=true,xft:Droid Sans Mono for Powerline:pixelsize=14:antialias=true
!URxvt.italicFont:     xft:Roboto Mono for Powerline:regular:pixelsize=14:antialias=true,xft:Droid Sans Mono for Powerline:pixelsize=14:antialias=true
!URxvt.boldItalicFont: xft:Roboto Mono for Powerline:regular:pixelsize=14:antialias=true,xft:Droid Sans Mono for Powerline:pixelsize=14:antialias=true

!URxvt.font: xft:Roboto Mono for Powerline:size=14,xft:Roboto Mono:size=12
!URxvt.boldFont: xft:Roboto Mono for Powerline:bold:size=14,xft:Roboto Mono:bold:size=12
!URxvt.italicFont: xft:Roboto Mono for Powerline:italic:size=14,xft:Roboto Mono:italic:size=12
!URxvt.bolditalicFont: xft:Roboto Mono for Powerline:bold:italic:size=14, xft:Roboto Mono:bold:italic:size=12

URxvt.font: xft:Hack-Regular:size=14
URxvt.boldFont: xft:Hack-Bold:bold:size=14
URxvt.italicFont: xft:Hack-Italic:italic:size=14
URxvt.bolditalicFont: xft:Hack-BoldItalic:bold:italic:size=14

!URxvt.font: -*-rissole-*
!URxvt.boldFont: -*-rissole-*
!URxvt.italicFont: -*-rissole-*
!URxvt.boldItalicfont: -*-rissole-*

Xft.dpi:                        96
Xft.antialias:                  1
Xft.autohint:                   1
Xft.hinting:                    1
Xft.hintstyle:                  hintslight
Xft.lcdfilter:                  lcdlight
Xft.rgba:                       rgb

URxvt*termName:                 rxvt-256color
URxvt.depth:                    32
URxvt*lineSpace:                0
URxvt.letterSpace:              0
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt*geometry:                 65x17
URxvt.scrollBar:                false
URxvt*loginShell:               true
URxvt.internalBorder:           25
URxvt.perl-ext:	                default,matcher
URxvt.url-launcher:             chromium
URxvt.matcher.button:           1
!URxvt.cursorBlink:              true
URxvt.saveline: 2048

rofi.color-enabled: true
rofi.color-window: #141021, #c16772, #9568a1
rofi.color-normal: #141021, #ffeefc, #312842, #c16772, #ffeefc
rofi.color-active: #141021, #c05c47, #312842, #c05c47, #ffeefc
rofi.color-urgent: #141021, #e4b226, #312842, #e4b226, #ffeefc
!rofi.font: Roboto\ Mono 12
rofi.font: Hack 12
rofi.width: 800
rofi.padding: 50
rofi.bw: 3
rofi.separator-style: solid
rofi.hide-scrollbar: true

